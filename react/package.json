{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client --port 8087",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "totaal": "npm run free-port && concurrently \"pip install -r ../requirements.txt\" \"py ../manage.py runserver 8000\" \"npm install\" \"npm start\"",
    "free-port": "powershell -Command \"Get-NetTCPConnection -LocalPort 8087 -ErrorAction SilentlyContinue | Select-Object -ExpandProperty OwningProcess | ForEach-Object { Stop-Process -Id $_ -Force }\" || echo Port 8087 is free"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.2",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-picker/picker": "^2.8.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.5",
    "expo": "~50.0.20",
    "expo-font": "~11.10.3",
    "expo-status-bar": "~1.12.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.75.2",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-progress": "^5.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "~0.19.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^8.2.2"
  },
  "private": true
}
